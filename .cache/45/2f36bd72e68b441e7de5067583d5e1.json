 USE MORE THAN ONCE\r\n\t\t// const promise = petfinder.breed.list({ animal: 'dog' }); //RETURNS A PROMISE - goes out on API and gets what you asked for\r\n\t\t// promise.then(console.log, console.error);\r\n\t\tpetfinder.pet.find({ output: 'full', location: 'Seattle, WA' }).then((data) => {\r\n\t\t\tlet pets;\r\n\r\n\t\t\tif (data.petfinder.pets && data.petfinder.pets.pet) {\r\n\t\t\t\tif (Array.isArray(data.petfinder.pets.pet)) {\r\n\t\t\t\t\t//IF YOU FIND MULTIPLE RESULTS RETURN AS FOUND\r\n\t\t\t\t\tpets = data.petfinder.pets.pet;\r\n\t\t\t\t} else {\r\n\t\t\t\t\tpets = [ data.petfinder.pets.pet ]; //IF YOU ONLY FIND ONE ANIMAL, RETURN THAT ANIMAL AS A SINGLE OBJECT IN AN ARRAY\r\n\t\t\t\t}\r\n\t\t\t} else {\r\n\t\t\t\tpets = []; //IF YOU FIND NO ANIMALS RETURN AN EMPTY ARRAY\r\n\t\t\t}\r\n\r\n\t\t\tthis.setState({\r\n\t\t\t\tpets //pets: pets\r\n\t\t\t});\r\n\t\t});\r\n\t}\r\n\t// \t\treturn React.createElement('div', {}, [\r\n\t// \t\t\tReact.createElement('h1', { onClick: this.handleTitleClick }, 'Adopt Me!'),\r\n\t// \t\t\tReact.createElement(Pet, {\r\n\t// \t\t\t\tname: 'Luna',\r\n\t// \t\t\t\tanimal: 'Dog',\r\n\t// \t\t\t\tbreed: 'Havanese'\r\n\t// \t\t\t}),\r\n\t// \t\t\tReact.createElement(Pet, {\r\n\t// \t\t\t\tname: 'Pepper',\r\n\t// \t\t\t\tanimal: 'Bird',\r\n\t// \t\t\t\tbreed: 'Cockatiel'\r\n\t// \t\t\t}),\r\n\t// \t\t\tReact.createElement(Pet, {\r\n\t// \t\t\t\tname: 'Doink',\r\n\t// \t\t\t\tanimal: 'Cat',\r\n\t// \t\t\t\tbreed: 'Mixed Breed'\r\n\t// \t\t\t})\r\n\t// \t\t]);\r\n\r\n\trender() {\r\n\t\treturn (\r\n\t\t\t<div className=\"search\">\r\n\t\t\t\t{this.state.pets.map((pet) => {\r\n\t\t\t\t\tlet breed;\r\n\r\n\t\t\t\t\tif (Array.isArray(pet.breeds.breed)) {\r\n\t\t\t\t\t\t//some pet breeds are double barrelled, this function differentiates so you dont get weird data pulled through\r\n\t\t\t\t\t\tbreed = pet.breeds.breed.join(', ');\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tbreed = pet.breeds.breed;\r\n\t\t\t\t\t}\r\n\t\t\t\t\treturn (\r\n\t\t\t\t\t\t<Pet\r\n\t\t\t\t\t\t\tkey={pet.id}\r\n\t\t\t\t\t\t\tanimal={pet.animal}\r\n\t\t\t\t\t\t\tname={pet.name}\r\n\t\t\t\t\t\t\tbreed={breed}\r\n\t\t\t\t\t\t\tmedia={pet.media}\r\n\t\t\t\t\t\t\tlocation={`${pet.contact.city}, ${pet.contact.state}`}\r\n\t\t\t\t\t\t/>\r\n\t\t\t\t\t);\r\n\t\t\t\t})}\r\n\t\t\t</div>\r\n\t\t);\r\n\t}\r\n}\r\n\r\n// ReactDOM.render(React.createElement(App, {}, null), document.getElementById('root')); ORIGINAL RENDER FUNCTION USED WITH react.createElement AT TOP OF PAGE\r\n\r\nexport default Results;\r\n"},"lineCount":null}},"hash":"071f861c21d1e5af8d1eb126a345cb28","cacheData":{"env":{"API_KEY":"ada57b9a9d27871d90b0daeb03dab13f","API_SECRET":"a371e8476acdf2762ce6a0503357be9c"}}}